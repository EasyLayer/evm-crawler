{
  "name": "@easylayer/evm-crawler",
  "version": "0.0.2",
  "description": "",
  "type": "commonjs",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "exports": {
    ".": {
      "module": "./dist/esm/index.mjs",
      "default": "./dist/index.js"
    },
    "./dist/*": {
      "types": "./dist/*.d.ts"
    }
  },
  "files": [
    "dist",
    ".env.example",
    "DOCS.md",
    "LICENSE"
  ],
  "bugs": {
    "url": "https://github.com/easylayer/evm-crawler/issues"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/easylayer/evm-crawler.git"
  },
  "scripts": {
    "clear": "yarn run clear:dist && yarn run clear:node_modules",
    "clear:node_modules": "rimraf node_modules",
    "clear:dist": "rimraf dist",
    "build:esm": "rollup --config",
    "build:cjs": "tsc -b ./tsconfig.cjs.json",
    "build": "yarn build:cjs && yarn build:esm",
    "prepublishOnly": "yarn clear:dist && yarn build",
    "test:unit": "jest --maxWorkers=1 -c ./jest.config.unit.mjs",
    "lint": "eslint -c eslint.config.mjs \"src/**/*.ts\"",
    "lint:fix": "eslint -c eslint.config.mjs \"src/**/*.ts\" --fix",
    "format": "prettier -c prettier.config.mjs --write \"src/**/*.ts\"",
    "docs:check": "ts-node scripts/check-docs.ts",
    "docs:website:generate": "ts-node scripts/generate-docs.ts"
  },
  "author": "Yaroslav Pavliuk",
  "license": "",
  "packageManager": "yarn@4.5.0",
  "dependencies": {
    "@easylayer/common": "0.9.2",
    "@easylayer/evm": "0.9.2",
    "@nestjs/common": "10.2.10",
    "@nestjs/core": "10.2.10",
    "@nestjs/microservices": "10.2.10",
    "@nestjs/platform-express": "10.2.10",
    "check-node-version": "4.2.1",
    "class-transformer": "0.5.1",
    "class-transformer-validator": "0.9.1",
    "class-validator": "0.14.0",
    "class-validator-jsonschema": "5.0.0",
    "commander": "11.1.0",
    "dotenv": "16.4.5",
    "fs-extra": "11.2.0",
    "piscina": "4.1.0",
    "reflect-metadata": "0.1.14",
    "rxjs": "7.8.1",
    "uuid": "9.0.1"
  },
  "devDependencies": {
    "@nestjs/testing": "10.2.10",
    "@rollup/plugin-typescript": "11.1.6",
    "@types/commander": "2.12.2",
    "@types/fs-extra": "11.0.4",
    "@types/jest": "29.5.11",
    "@types/node": "20.10.4",
    "@types/uuid": "9.0.7",
    "chalk": "4.1.2",
    "esbuild": "0.25.4",
    "jest": "29.7.0",
    "rollup": "4.17.2",
    "ts-jest": "29.1.1",
    "ts-node": "10.9.2",
    "typescript": "5.3.3"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "engines": {
    "node": ">=17.0.0 <=22.x.x",
    "npm": ">=8.0.0"
  }
}
